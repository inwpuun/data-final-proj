    "affiliation": [
        {
            "affiliation-city": "Nakhon Ratchasima",
            "@id": "60092136",
            "affilname": "Rajamangala University of Technology Isan",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60092136",
            "affiliation-country": "Thailand"
