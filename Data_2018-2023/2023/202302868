            "affiliation-city": "Nakhon Ratchasima",
            "@id": "112649034",
            "affilname": "Synchrotron Light Research Institute (Public Organization)",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/112649034",
            "affiliation-country": "Thailand"
