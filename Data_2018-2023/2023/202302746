    "affiliation": {
        "affiliation-city": "Bangkok",
        "@id": "60199582",
        "affilname": "Center of Excellence on Petrochemical and Materials Technology",
        "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60199582",
        "affiliation-country": "Thailand"
