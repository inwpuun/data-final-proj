            "affiliation-city": "Piscataway",
            "@id": "60007884",
            "affilname": "Rutgers Environmental and Occupational Health Sciences Institute",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60007884",
            "affiliation-country": "United States"
