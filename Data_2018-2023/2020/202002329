                    {"collaboration": {
                        "@seq": "13",
                        "@collaboration-instance-id": "2004346959-5ddc0f948ce12f9114cd8f52252ab562",
                        "ce:text": "on behalf of the ACC working group",
                        "ce:indexed-name": "on behalf of the ACC working group"
