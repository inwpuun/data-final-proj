                        "@collaboration-instance-id": "S1879406818303631-686180d27f310a4f84d2f05e8bec515d",
                        "ce:text": "on behalf of Thai Lymphoma Study Group",
                        "ce:indexed-name": "on behalf of Thai Lymphoma Study Group"
