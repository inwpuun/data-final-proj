                    {"collaboration": {
                        "@seq": "34",
                        "@collaboration-instance-id": "2010856492-4b58d43455183416bf8586e62aff56fb",
                        "ce:text": "SEA-AKI study group",
                        "ce:indexed-name": "SEA-AKI study group"
