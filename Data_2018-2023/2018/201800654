        "affiliation-city": "Seoul",
        "@id": "60115878",
        "affilname": "Yonsei University College of Dentistry",
        "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60115878",
        "affiliation-country": "South Korea"
