            "affiliation-city": "Fairfield",
            "@id": "60022249",
            "affilname": "Sacred Heart University Fairfield",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60022249",
            "affiliation-country": "United States"
