        {
            "affiliation-city": "Bangkok",
            "@id": "60091516",
            "affilname": "Center of Excellence on Hazardous Substance Management",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60091516",
            "affiliation-country": "Thailand"
