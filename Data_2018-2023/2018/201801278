                    {"collaboration": {
                        "@seq": "10",
                        "@collaboration-instance-id": "S0720048X18301475-6766a9a5147b44200cb06822d0ebb0f1",
                        "ce:text": "DIMITRA Research Group",
                        "ce:indexed-name": "DIMITRA Research Group"
