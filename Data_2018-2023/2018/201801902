        {
            "affiliation-city": "Nakhon Ratchasima",
            "@id": "60013120",
            "affilname": "Maharaj Nakhon Ratchasima Hospital",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60013120",
            "affiliation-country": "Thailand"
