            "affiliation-city": "Czestochowa",
            "@id": "60009913",
            "affilname": "Częstochowa University Of Technology",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60009913",
            "affiliation-country": "Poland"
