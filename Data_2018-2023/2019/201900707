            "affiliation-city": "Bangkok",
            "@id": "60194933",
            "affilname": "Chandrakasem Rajabhat University",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60194933",
            "affiliation-country": "Thailand"
        },
