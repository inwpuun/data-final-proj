        {
            "affiliation-city": "Fukuoka",
            "@id": "110245452",
            "affilname": "Chikushi Hospital",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/110245452",
            "affiliation-country": "Japan"
        }
