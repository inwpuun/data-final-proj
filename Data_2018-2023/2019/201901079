        "affiliation-city": "Bangkok",
        "@id": "60022183",
        "affilname": "King Chulalongkorn Memorial Hospital",
        "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60022183",
        "affiliation-country": "Thailand"
