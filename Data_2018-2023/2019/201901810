            "affiliation-city": "Bangrak, Bangkok",
            "@id": "128801573",
            "affilname": "Saladaeng Physiotherapy Clinic",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/128801573",
            "affiliation-country": "Thailand"
