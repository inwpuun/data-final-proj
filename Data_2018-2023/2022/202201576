    "affiliation": [
        {
            "affiliation-city": "Jinan",
            "@id": "60261038",
            "affilname": "Shandong Women\u2019s University",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60261038",
            "affiliation-country": "China"
