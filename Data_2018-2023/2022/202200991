                            "@seq": "156",
                            "@collaboration-instance-id": "2018280180-2f6bb867eb69bcd174036b9331db68db",
                            "ce:text": "APASL ACLF Research Consortium (AARC) for APASL ACLF Working Party",
                            "ce:indexed-name": "APASL ACLF Research Consortium (AARC) for APASL ACLF Working Party"
