                    {"collaboration": {
                        "@seq": "11",
                        "@collaboration-instance-id": "2019091969-b190390579b93f4fa561ec2887af66c7",
                        "ce:text": "HIV-NAT 006/207 study team",
                        "ce:indexed-name": "HIV-NAT 006/207 study team"
