                        "@seq": "25",
                        "@collaboration-instance-id": "OB2BibRecID-943370782-eeedae43ecb8b62a2a8af3f4e58f21c5-1",
                        "ce:text": "Improve Sudden Cardiac Arrest Bridge Investigators",
