                        "@seq": "19",
                        "@collaboration-instance-id": "2016768603-73437dac024009ce0fc330d966995111",
                        "ce:text": "Thai Pediatric Oncology Neuroblastoma Working Group",
                        "ce:indexed-name": "Thai Pediatric Oncology Neuroblastoma Working Group"
