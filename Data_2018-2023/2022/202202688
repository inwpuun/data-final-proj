                        "@seq": "81",
                        "@collaboration-instance-id": "2017548381-2318c5bd3c8fbe4e07d4291d204d175d",
                        "ce:text": "IeDEA Asia-Pacific",
                        "ce:indexed-name": "IeDEA Asia-Pacific"
