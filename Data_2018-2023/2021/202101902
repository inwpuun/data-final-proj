                                "ce:indexed-name": "Hasselquist N.J."
                            },
                            "@author-instance-id": "S0378112721001900-e2302e2e718deaed7049f7e504a67896",
                            "@seq": "11",
                            "ce:initials": "N.J.",
                            "@_fa": "true",
                            "@type": "auth",
