                        "collaboration": {
                            "@seq": "328",
                            "@collaboration-instance-id": "2010176402-7344f82b5a938682bdc176cd6859c2dc",
                            "ce:text": "the ODYSSEY Trial Team",
                            "ce:indexed-name": "the ODYSSEY Trial Team"
