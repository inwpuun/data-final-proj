        {
            "affiliation-city": "Rockhampton",
            "@id": "60031003",
            "affilname": "CQUniversity Australia",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60031003",
            "affiliation-country": "Australia"
