            "affiliation-city": "Brussels",
            "@id": "60026810",
            "affilname": "Vrije Universiteit Brussel",
            "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60026810",
            "affiliation-country": "Belgium"
