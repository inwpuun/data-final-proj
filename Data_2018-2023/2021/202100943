                    {"collaboration": {
                        "@seq": "10",
                        "@collaboration-instance-id": "2013158527-841109d0792f5b2632c5cf4c284ee3ed",
                        "ce:text": "for the Thai Aplastic Anemia Study Group",
                        "ce:indexed-name": "for the Thai Aplastic Anemia Study Group"
