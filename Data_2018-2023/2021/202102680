                        "@seq": "81",
                        "@collaboration-instance-id": "2011201468-cfcf43cc1f3d89a988152fc8381c5263",
                        "ce:text": "on behalf of the T-cell Lymphoma Project",
                        "ce:indexed-name": "on behalf of the T-cell Lymphoma Project"
